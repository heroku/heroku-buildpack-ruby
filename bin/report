#!/usr/bin/env bash

set -euo pipefail

CACHE_DIR="${2}"
# The absolute path to the root of the buildpack.
BUILDPACK_DIR=$(cd "$(dirname "$(dirname "${BASH_SOURCE[0]}")")" && pwd)

# Initialize metrics for bin/report
source "${BUILDPACK_DIR}/bin/support/metrics.sh"
metrics::init "${CACHE_DIR}"
metrics::print
